{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext"],
    "module": "commonjs",
    "skipLibCheck": true,
    "strict": true,
    "outDir": "dist",
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUncheckedIndexedAccess": true,
  },
  "include": ["src", "**/*.ts", "prisma"],
  "exclude": ["src/__tests__", "node_modules"]
}
